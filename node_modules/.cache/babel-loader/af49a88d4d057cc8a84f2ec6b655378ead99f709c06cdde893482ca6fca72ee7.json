ue={item} onChange={e => setItem(e.target.value)} /> <br />\r\n      <input placeholder=\"Link\" value={link} onChange={e => setLink(e.target.value)} /> <br />\r\n      <button onClick={saveDesejo} style={{ marginLeft: '10px' }}>\r\n        {editId ? 'Atualizar' : 'Adicionar'}\r\n      </button>\r\n\r\n      <ul>\r\n        {desejos.map(d => (\r\n          <li key={d.id} style={{ marginTop: '10px' }}>\r\n            {d.item} - <a href={d.link} target=\"_blank\" rel=\"noopener noreferrer\">{d.link}</a>\r\n            <button onClick={() => editDesejo(d)} style={{ marginLeft: '10px' }}>Editar</button> \r\n            <button onClick={() => removeDesejo(d.id)} style={{ marginLeft: '5px' }}>Remover</button>\r\n          </li>\r\n        ))}\r\n      </ul>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default Desejos;\r\n"],"mappings":"AAAA,OAASA,QAAQ,CAAEC,SAAS,KAAQ,OAAO,CAC3C,MAAO,CAAAC,KAAK,KAAM,OAAO,CACzB,MAAO,eAAe,CAAC,OAAAC,GAAA,IAAAC,IAAA,CAAAC,IAAA,IAAAC,KAAA,yBAGvB,QAAS,CAAAC,OAAOA,CAAA,CAAG,CACjB,KAAM,CAACC,OAAO,CAAEC,UAAU,CAAC,CAAGT,QAAQ,CAAC,EAAE,CAAC,CAC1C,KAAM,CAACU,IAAI,CAAEC,OAAO,CAAC,CAAGX,QAAQ,CAAC,EAAE,CAAC,CACpC,KAAM,CAACY,IAAI,CAAEC,OAAO,CAAC,CAAGb,QAAQ,CAAC,EAAE,CAAC,CACpC,KAAM,CAACc,MAAM,CAAEC,SAAS,CAAC,CAAGf,QAAQ,CAAC,IAAI,CAAC,CAE1CC,SAAS,CAAC,IAAM,CACde,YAAY,CAAC,CAAC,CAChB,CAAC,CAAE,EAAE,CAAC,CAEN,KAAM,CAAAA,YAAY,CAAGA,CAAA,GAAM,CACzBd,KAAK,CAACe,GAAG,CAAC,+BAA+B,CAAC,CACvCC,IAAI,CAACC,GAAG,EAAIV,UAAU,CAACU,GAAG,CAACC,IAAI,CAAC,CAAC,CACjCC,KAAK,CAACC,GAAG,EAAIC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC,CAAC,CACnC,CAAC,CAED,KAAM,CAAAG,UAAU,CAAGA,CAAA,GAAM,CACvB,GAAI,CAACf,IAAI,EAAI,CAACE,IAAI,CAAE,OAEpB,GAAIE,MAAM,CAAE,CACVZ,KAAK,CAACwB,GAAG,kCAAAC,MAAA,CAAkCb,MAAM,EAAI,CAAEJ,IAAI,CAAEE,IAAK,CAAC,CAAC,CACjEM,IAAI,CAAC,IAAM,CACVP,OAAO,CAAC,EAAE,CAAC,CACXE,OAAO,CAAC,EAAE,CAAC,CACXE,SAAS,CAAC,IAAI,CAAC,CACfC,YAAY,CAAC,CAAC,CAChB,CAAC,CAAC,CACN,CAAC,IAAM,CACLd,KAAK,CAAC0B,IAAI,CAAC,+BAA+B,CAAE,CAAElB,IAAI,CAAEE,IAAK,CAAC,CAAC,CACxDM,IAAI,CAAC,IAAM,CACVP,OAAO,CAAC,